local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()
local win = DiscordLib:Window("Atomic")
local serv = win:Server("Bitcoin-Miner", "")
local btns = serv:Channel("Home")

DiscordLib:Notification("Attention!", "Bitcoin Miner Loaded!", "Okay!")

btns:Button("Click for more info", function()
DiscordLib:Notification("Attention!", "Our Discord Server! | https://discord.gg/AD4JJSFYpj", "Okay!")
end)

btns:Seperator()

local tgls = serv:Channel("Automation")

tgls:Button("Exchange Bitcoins!", function()

    yes = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(246.23, 6.29, 89.88)
    wait(0.5)
    local args = {
        [1] = true
    }
    game:GetService("ReplicatedStorage").Events.ExchangeMoney:FireServer(unpack(args))
    wait(0.5)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = yes

end)

tgls:Toggle("Auto Claim Boost Star",false, function(FreeStar)
	_G.Freess = FreeStar
	while _G.Freess do
        game:GetService("ReplicatedStorage").Events.ClaimFreeBoostStar:FireServer()
        wait(3)
end
end)

tgls:Seperator()

tgls:Label("Change Mining Algotihms!")

tgls:Button("Change Algorithm 1", function()
local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage").Events.AlgoChang:FireServer(unpack(args))
end)

tgls:Button("Change Algorithm 2", function()
    local args = {
        [1] = 2
    }
    
    game:GetService("ReplicatedStorage").Events.AlgoChang:FireServer(unpack(args))
    end)

    tgls:Button("Change Algorithm 3", function()
        local args = {
            [1] = 3
        }
        
        game:GetService("ReplicatedStorage").Events.AlgoChang:FireServer(unpack(args))
        end)

tgls:Button("Change Algorithm 4", function()
    local args = {
        [1] = 4
    }
            
    game:GetService("ReplicatedStorage").Events.AlgoChang:FireServer(unpack(args))
end)      

