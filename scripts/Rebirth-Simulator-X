local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/IreXion-UI-Library/main/Revamp/IreXion%20UI%20Library"))()
local Gui = Library:AddGui({
    Title = {"Atomic", "Rebirth Simulator X"},
    ThemeColor = Color3.fromRGB(0, 255, 0),
    ToggleKey = Enum.KeyCode.RightShift,
})


function getCurrentPlayerPOS()
	local plyr = game.LocalPlayer;
	if plyr.Character then
		return plyr.Character.HumanoidRootPart.Position;
	end
	return false;
end

function teleportTO(placeCFrame)
	local plyr = game.Players.LocalPlayer;
	if plyr.Character then
		plyr,Character.HumanoidRootPart.CFrame = placeCFrame
	end
end

function teleportMap(map)
	if game:GetService("Workspace").Maps:FindFirstChild(map) then
		teleportTo(game:GetService("Workspace").Maps[map].Teleport.CFrame)
	end
end
	




local Tab = Gui:AddTab("Rebirth Simulator")

local tgls = Tab:AddCategory("Automation")

local ReplicatedStorage = game:GetService("ReplicatedStorage")

tgls:AddToggle("Auto-Clicker",false, function(mama)
		_G.AutoClicker = mama
		while _G.AutoClicker do
			
        ReplicatedStorage.Events.Click3:FireServer()
		wait(0.0002)
end
end)

tgls:AddButton("Upgrade Click-Multiplier", function()
local args = {
    [1] = "ClickMultiplier"
}

game:GetService("ReplicatedStorage").Functions.Upgrade:InvokeServer(unpack(args))
end)


tgls:AddToggle("2x Click Potion",false, function(Pot1)
	_G.Potion1 = Pot1
	while _G.Potion1 do
		local args = {
			[1] = "x2Clicks"
		}
		
		game:GetService("ReplicatedStorage").Events.Potion:FireServer(unpack(args))
		wait(600000)
	end
end)

