local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()
local win = DiscordLib:Window("Atomic")
local serv = win:Server("Rebirth-Simulator-X", "")
local btns = serv:Channel("Home")

DiscordLib:Notification("Attention!", "Rebirth Simulator X Loaded!", "Okay!")

btns:Button("Click for more info", function()
DiscordLib:Notification("Attention!", "Our Discord Server! | https://discord.gg/AD4JJSFYpj", "Okay!")
end)

btns:Seperator()

local tgls = serv:Channel("Clicking")

tgls:Toggle("Auto-Clicker",false, function(mama)
		_G.AutoClicker = mama
		while _G.AutoClicker do
			game:GetService("ReplicatedStorage").Events.Click3:FireServer()
			print("Clicked Once!")
			wait(0.001)
end
end)

tgls:Seperator()

tgls:Button("Upgrade Click-Multiplier", function()
local args = {
    [1] = "ClickMultiplier"
}

game:GetService("ReplicatedStorage").Functions.Upgrade:InvokeServer(unpack(args))
end)

local sldrs = serv:Channel("Rebirth-Automation")

sldrs:Label("Rebirths again as soon as you can afford it!")

sldrs:Toggle("Rebirth-Once",false, function(mama1)
	_G.Rebirth1 = mama1
	while _G.Rebirth1 do
	local args = {
		[1] = 1
	}
	game:GetService("ReplicatedStorage").Events.Rebirth:FireServer(unpack(args))
	wait(0.3)
end
end)

