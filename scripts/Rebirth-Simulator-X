local drops = serv:Channel("Teleports")

drops:Label("Click once to teleport!")

drops:Button("Spawn", function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(167.3, 4.25, 60.5)
	DiscordLib:Notification("Success!", "Teleported to spawn!", "Okay!")
end)

drops:Button("Forest", function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-386.9, 29.9, 174.8)
	DiscordLib:Notification("Success!", "Teleported to forest!", "Okay!")
end)

drops:Button("Beach", function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-517.28, 34.8, 458.3)
	DiscordLib:Notification("Success!", "Teleported to beach!", "Okay!")
